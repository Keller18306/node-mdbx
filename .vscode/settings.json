    "launch": {
        "version": "0.2.0",
        "configurations": [
            {
                "type": "lldb",
                "request": "launch",
                "name": "Debug",
                "program": "/usr/bin/node",
                "args": [
                    "--expose-gc",
                    "index.js"
                ],
                "cwd": "${workspaceFolder}",
                // "env": {
                //     "NODE_OPTIONS": "-r ts-node/register --inspect"
                // }
            }
        ],
        "compounds": []
    },
    "editor.formatOnSaveMode": "file"
}